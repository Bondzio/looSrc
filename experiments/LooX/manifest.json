{
  "manifest_version": 2,
  "name": "Save Asset To",
  "description": "This extension saves assets to a server.",
  "version": "1.0",

  "permissions": [
    "http://loooping.ch/",
    "contextMenus"
  ],
  "background": {
    "scripts": ["uploadImage.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}